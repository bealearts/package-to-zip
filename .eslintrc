{
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "no-use-before-define": ["off"],
        "no-unused-vars": ["warn"],
        "no-prototype-builtins": ["off"],
        "no-underscore-dangle": ["off"],
        "class-methods-use-this": ["off"],
        "import/no-dynamic-require": ["off"]
    }
}
