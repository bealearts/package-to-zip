    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "decompress-zip": "^0.3.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
